{
  "tasks": {
    "npm": "deno run -A --no-check https://deno.land/x/npm_bridge@0.0.5/npm.ts",
    "npx": "deno run -A --no-check https://deno.land/x/npm_bridge@0.0.5/npx.ts",
    "run": "deno run --allow-read --allow-env --unstable main.ts",
    "copyfiles": "deno task npx -A copyfiles",
    "mkdirp": "deno task npx -A mkdirp"
  },
  "importMap": "./npm_deps/import_map.json"
}
